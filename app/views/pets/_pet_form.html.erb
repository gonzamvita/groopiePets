<%= form_for @pet do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :breed %>
  <%= form.text_field :breed %>
  <br>
  <%= form.label :age %>
  <%= form.text_field :age %>
  <br>
  <%= form.label :photo %>
  <%= form.file_field :photo %>
  <br>
  <%= form.submit 'Save' %>
<% end %>
